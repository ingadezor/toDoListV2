<%- include('partials/header.ejs') %>

<main>
    <div id="title"><h2><%= title %></h2></div>
   


    <div id="list">
        <ul>
            <% for(let i=0; i<list.length; i++){ %>
                
                <li> <input type="checkbox"> <%= list[i] %> </li>
            <% } %>
            
        </ul>
    </div>


    <form action="/submit" method="POST">
        <input type="text" name="newItem" value="" placeholder="New Item" required>
        <button type="submit" name="listType" value=<%= title %>>+</button>
    </form>
    
    
</main>


</body>
</html>